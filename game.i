void updatePieceParent(pieceParent* pp) {
    if (pp->selected > 0) {
        pp->vOffset += vselDel;
        pp->hOffset += hselDel;
        pp->screenRow = pp->worldRow * 8 - vOff + pp->vOffset;
        pp->screenCol = pp->worldCol * 8 - hOff + pp->hOffset;
    } else {
        pp->screenRow = pp->worldRow * 8 - vOff + pp->vOffset;
        pp->screenCol = pp->worldCol * 8 - hOff + pp->hOffset;
    }
}
